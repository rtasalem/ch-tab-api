{
  "name": "ch-tab-api",
  "version": "0.2.1",
  "description": "GraphQL API for querying and mutating data stored in Azure Cosmos DB for the Take A Bao website.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run test:lint",
    "test": "jest --runInBand --forceExit",
    "test:watch": "jest --coverage=false --onlyChanged --watch --runInBand",
    "test:lint": "standard",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js --coverage=false --onlyChanged --watch --runInBand --no-cache",
    "start:watch": "nodemon --inspect=0.0.0.0 --ext js --legacy-watch app/index.js",
    "start:debug": "nodemon --inspect-brk=0.0.0.0 --ext js --legacy-watch app/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtasalem/ch-tab-api.git"
  },
  "keywords": [
    "graphql",
    "cosmosdb",
    "mongodb"
  ],
  "author": "Rana Salem (https://github.com/rtasalem)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rtasalem/ch-tab-api/issues"
  },
  "homepage": "https://github.com/rtasalem/ch-tab-api#readme",
  "devDependencies": {
    "nodemon": "3.1.4",
    "standard": "17.1.0"
  },
  "dependencies": {
    "@as-integrations/hapi": "2.0.1",
    "@azure/cosmos": "4.1.0",
    "@hapi/hapi": "21.3.10",
    "express": "4.19.2",
    "joi": "17.13.3",
    "mongodb": "6.8.0"
  }
}
